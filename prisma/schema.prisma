generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Phases {
  id            Int        @id @default(autoincrement())
  program_id    Int
  number        String    
  allocation    String
  from_year     Int
  to_year       Int
}