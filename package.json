{
  "name": "mockapi",
  "version": "1.0.0",
  "description": "This is a mock api for testing",
  "homepage": "https://github.com/kjsninja/mockapi#readme",
  "bugs": {
    "url": "https://github.com/kjsninja/mockapi/issues"
  },
  "repository": {
